          section
            h2 Fetch API Example
            pre
              code(data-trim data-noescape).
                // Modern way to make HTTP requests
                async function fetchUsers() {
                  try {
                    const response = await fetch('https://jsonplaceholder.typicode.com/users');
                    
                    if (!response.ok) {
                      throw new Error(`HTTP error! status: ${response.status}`);
                    }
                    
                    const users = await response.json();
                    return users;
                  } catch (error) {
                    console.error('Failed to fetch users:', error);
                    throw error;
                  }
                }

                // Usage
                fetchUsers()
                  .then(users => {
                    users.forEach(user => console.log(user.name));
                  })
                  .catch(error => console.error(error));

          section
            h2 Best Practices
            ul
              li.fragment Use #[code const] by default, #[code let] when reassigning
              li.fragment Prefer arrow functions for callbacks
              li.fragment Use async/await over .then() chains
              li.fragment Always handle errors (try/catch, .catch())
              li.fragment Use strict equality (#[code ===]) over loose (#[code ==])
              li.fragment Keep functions small and focused
              li.fragment Use meaningful variable and function names
              li.fragment Leverage modern ES6+ features

          section
            h2 Common Patterns
            .flex
              .half
                h3 Module Pattern
                pre
                  code(data-trim data-noescape).
                    const userManager = (() => {
                      let users = [];
                      
                      return {
                        addUser(user) {
                          users.push(user);
                        },
                        getUsers() {
                          return [...users];
                        },
                        getUserCount() {
                          return users.length;
                        }
                      };
                    })();
              .half
                h3 Observer Pattern
                pre
                  code(data-trim data-noescape).
                    class EventEmitter {
                      constructor() {
                        this.events = {};
                      }
                      
                      on(event, listener) {
                        if (!this.events[event]) {
                          this.events[event] = [];
                        }
                        this.events[event].push(listener);
                      }
                      
                      emit(event, data) {
                        if (this.events[event]) {
                          this.events[event].forEach(listener => 
                            listener(data)
                          );
                        }
                      }
                    }

