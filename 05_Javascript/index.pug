        section#intro2
          section
            h2 What is JavaScript?
            ul
              li.fragment High-level, interpreted programming language
              li.fragment Originally created for web browsers (1995)
              li.fragment Now runs everywhere: servers, mobile, desktop
              li.fragment Dynamic, weakly typed, prototype-based
              li.fragment One of the core technologies of the web

          section
            h2 JavaScript vs ECMAScript
            ul
              li.fragment #[strong ECMAScript] is the specification
              li.fragment #[strong JavaScript] is an implementation
              li.fragment ES6/ES2015 introduced major improvements
              li.fragment Annual releases (ES2016, ES2017, etc.)
              li.fragment Modern browsers support most ES6+ features

          section
            h2 Where JavaScript Runs
            .flex
              .half
                h3 Client-side
                ul
                  li.fragment Web browsers
                  li.fragment Mobile apps (React Native)
                  li.fragment Desktop apps (Electron)
              .half
                h3 Server-side
                ul
                  li.fragment Node.js
                  li.fragment Serverless functions
                  li.fragment APIs and microservices

        section#grammar
          section
            h2 Grammar and Types
            h3 Variable Declarations

          section
            h2 var vs let vs const
            .flex
              .half
                pre
                  code(data-trim data-noescape).
                    // var - function scoped, hoisted
                    var name = "John";
                    var name = "Jane"; // OK

                    // let - block scoped
                    let age = 25;
                    // let age = 30; // Error!

                    // const - block scoped, immutable
                    const PI = 3.14159;
                    // PI = 3; // Error!
              .half
                ul
                  li.fragment #[code var]: function scoped, can redeclare
                  li.fragment #[code let]: block scoped, no redeclaration
                  li.fragment #[code const]: block scoped, immutable binding
                  li.fragment Always prefer #[code const], then #[code let]

          section
            h2 Data Types
            .flex
              .half
                h3 Primitives
                ul
                  li.fragment #[code number]: 42, 3.14
                  li.fragment #[code string]: "Hello"
                  li.fragment #[code boolean]: true/false
                  li.fragment #[code undefined]: undefined
                  li.fragment #[code null]: null
                  li.fragment #[code symbol]: Symbol("id")
                  li.fragment #[code bigint]: 123n
              .half
                h3 Objects
                ul
                  li.fragment #[code object]: {}, []
                  li.fragment #[code function]: function() {}
                  li.fragment Everything else!

          section
            h2 Type Coercion Examples
            pre
              code(data-trim data-noescape).
                // Automatic type conversion
                "5" + 3        // "53" (string concat)
                "5" - 3        // 2 (number subtraction)
                "5" == 5       // true (loose equality)
                "5" === 5      // false (strict equality)
                
                // Falsy values
                if (!0) console.log("0 is falsy");
                if (!"") console.log("empty string is falsy");
                if (!null) console.log("null is falsy");
                if (!undefined) console.log("undefined is falsy");

        section#functions
          section
            h2 Functions

          section
            h2 Function Declaration vs Expression
            .flex
              .half
                h3 Declaration
                pre
                  code(data-trim data-noescape).
                    // Hoisted - can call before definition
                    console.log(add(2, 3)); // 5
                    
                    function add(a, b) {
                      return a + b;
                    }
              .half
                h3 Expression
                pre
                  code(data-trim data-noescape).
                    // Not hoisted
                    // console.log(subtract(5, 2)); // Error!
                    
                    const subtract = function(a, b) {
                      return a - b;
                    };

          section
            h2 Arrow Functions
            pre
              code(data-trim data-noescape).
                // Traditional function
                const multiply = function(a, b) {
                  return a * b;
                };

                // Arrow function - concise
                const multiply2 = (a, b) => a * b;

                // Arrow function - with block
                const divide = (a, b) => {
                  if (b === 0) throw new Error("Division by zero");
                  return a / b;
                };

                // Single parameter (parentheses optional)
                const square = x => x * x;

          section
            h2 Scope and Closures
            pre
              code(data-trim data-noescape).
                function createCounter() {
                  let count = 0;
                  
                  return function() {
                    count++;
                    return count;
                  };
                }

                const counter1 = createCounter();
                const counter2 = createCounter();

                console.log(counter1()); // 1
                console.log(counter1()); // 2
                console.log(counter2()); // 1 (separate closure)

          section
            h2 Parameters and Arguments
            pre
              code(data-trim data-noescape).
                // Default parameters
                function greet(name = "World") {
                  return `Hello, ${name}!`;
                }

                // Rest parameters
                function sum(...numbers) {
                  return numbers.reduce((total, num) => total + num, 0);
                }

                // Destructuring parameters
                function printUser({name, age, email}) {
                  console.log(`${name} (${age}): ${email}`);
                }

                printUser({name: "John", age: 30, email: "john@example.com"});

        section#control
          section
            h2 Control Flow

          section
            h2 Conditionals
            .flex
              .half
                h3 if/else
                pre
                  code(data-trim data-noescape).
                    const age = 18;
                    
                    if (age >= 21) {
                      console.log("Can drink");
                    } else if (age >= 18) {
                      console.log("Can vote");
                    } else {
                      console.log("Too young");
                    }

                    // Ternary operator
                    const status = age >= 18 ? "adult" : "minor";
              .half
                h3 switch
                pre
                  code(data-trim data-noescape).
                    const day = "monday";
                    
                    switch (day) {
                      case "monday":
                      case "tuesday":
                        console.log("Weekday");
                        break;
                      case "saturday":
                      case "sunday":
                        console.log("Weekend");
                        break;
                      default:
                        console.log("Unknown day");
                    }

          section
            h2 Error Handling
            pre
              code(data-trim data-noescape).
                function parseJSON(str) {
                  try {
                    const data = JSON.parse(str);
                    return { success: true, data };
                  } catch (error) {
                    console.error("Parse error:", error.message);
                    return { success: false, error: error.message };
                  } finally {
                    console.log("Parse attempt completed");
                  }
                }

                // Throwing custom errors
                function divide(a, b) {
                  if (b === 0) {
                    throw new Error("Division by zero is not allowed");
                  }
                  return a / b;
                }

        section#loops
          section
            h2 Loops and Iteration

          section
            h2 Basic Loops
            .flex
              .half
                h3 for loop
                pre
                  code(data-trim data-noescape).
                    for (let i = 0; i < 5; i++) {
                      console.log(i);
                    }

                h3 while loop
                pre
                  code(data-trim data-noescape).
                    let i = 0;
                    while (i < 5) {
                      console.log(i);
                      i++;
                    }
              .half
                h3 do...while loop
                pre
                  code(data-trim data-noescape).
                    let i = 0;
                    do {
                      console.log(i);
                      i++;
                    } while (i < 5);

                h3 break/continue
                pre
                  code(data-trim data-noescape).
                    for (let i = 0; i < 10; i++) {
                      if (i === 3) continue; // skip 3
                      if (i === 7) break;    // stop at 7
                      console.log(i);
                    }

          section
            h2 for...in vs for...of
            .flex
              .half
                h3 for...in (properties)
                pre
                  code(data-trim data-noescape).
                    const obj = {a: 1, b: 2, c: 3};
                    
                    for (const key in obj) {
                      console.log(key, obj[key]);
                    }
                    // Output: "a" 1, "b" 2, "c" 3

                    const arr = ["x", "y", "z"];
                    for (const index in arr) {
                      console.log(index); // "0", "1", "2"
                    }
              .half
                h3 for...of (values)
                pre
                  code(data-trim data-noescape).
                    const arr = ["x", "y", "z"];
                    
                    for (const value of arr) {
                      console.log(value);
                    }
                    // Output: "x", "y", "z"

                    const str = "hello";
                    for (const char of str) {
                      console.log(char); // "h", "e", "l", "l", "o"
                    }

        section#objects
          section
            h2 Objects and Classes

          section
            h2 Creating Objects
            .flex
              .half
                h3 Object Literals
                pre
                  code(data-trim data-noescape).
                    const person = {
                      name: "John",
                      age: 30,
                      greet() {
                        return `Hello, I'm ${this.name}`;
                      }
                    };

                    console.log(person.greet()); // Hello, I'm John

                h3 Constructor Functions
                pre
                  code(data-trim data-noescape).
                    function Person(name, age) {
                      this.name = name;
                      this.age = age;
                    }

                    const john = new Person("John", 30);
              .half
                h3 Object.create()
                pre
                  code(data-trim data-noescape).
                    const personProto = {
                      greet() {
                        return `Hello, I'm ${this.name}`;
                      }
                    };

                    const john = Object.create(personProto);
                    john.name = "John";
                    john.age = 30;

                h3 Property Access
                pre
                  code(data-trim data-noescape).
                    // Dot notation
                    person.name = "Jane";
                    
                    // Bracket notation
                    person["age"] = 25;
                    
                    // Dynamic properties
                    const prop = "email";
                    person[prop] = "jane@example.com";

          section
            h2 Classes (ES6+)
            pre
              code(data-trim data-noescape).
                class Animal {
                  constructor(name, species) {
                    this.name = name;
                    this.species = species;
                  }

                  speak() {
                    return `${this.name} makes a sound`;
                  }

                  static getKingdom() {
                    return "Animalia";
                  }
                }

                class Dog extends Animal {
                  constructor(name, breed) {
                    super(name, "Canis lupus");
                    this.breed = breed;
                  }

                  speak() {
                    return `${this.name} barks`;
                  }
                }

                const dog = new Dog("Rex", "Golden Retriever");
                console.log(dog.speak()); // Rex barks

          section
            h2 Getters and Setters
            pre
              code(data-trim data-noescape).
                class Circle {
                  constructor(radius) {
                    this._radius = radius;
                  }

                  get radius() {
                    return this._radius;
                  }

                  set radius(value) {
                    if (value < 0) {
                      throw new Error("Radius cannot be negative");
                    }
                    this._radius = value;
                  }

                  get area() {
                    return Math.PI * this._radius ** 2;
                  }
                }

                const circle = new Circle(5);
                console.log(circle.area); // 78.54

        section#modern
          section
            h2 Modern JavaScript Features

          section
            h2 Template Literals
            pre
              code(data-trim data-noescape).
                const name = "John";
                const age = 30;

                // Old way
                const message1 = "Hello, my name is " + name + " and I'm " + age + " years old.";

                // Template literals (ES6+)
                const message2 = `Hello, my name is ${name} and I'm ${age} years old.`;

                // Multi-line strings
                const html = `
                  <div class="user">
                    <h2>${name}</h2>
                    <p>Age: ${age}</p>
                  </div>
                `;

                // Tagged templates
                function highlight(strings, ...values) {
                  return strings.reduce((result, string, i) => {
                    return result + string + (values[i] ? `<mark>${values[i]}</mark>` : '');
                  }, '');
                }

                const highlighted = highlight`Name: ${name}, Age: ${age}`;

          section
            h2 Destructuring
            .flex
              .half
                h3 Array Destructuring
                pre
                  code(data-trim data-noescape).
                    const arr = [1, 2, 3, 4, 5];
                    
                    const [first, second, ...rest] = arr;
                    console.log(first); // 1
                    console.log(rest);  // [3, 4, 5]

                    // Swapping variables
                    let a = 1, b = 2;
                    [a, b] = [b, a];
                    console.log(a, b); // 2, 1
              .half
                h3 Object Destructuring
                pre
                  code(data-trim data-noescape).
                    const user = {
                      name: "John",
                      age: 30,
                      email: "john@example.com"
                    };

                    const {name, age, email} = user;
                    
                    // With aliases
                    const {name: userName, age: userAge} = user;
                    
                    // With defaults
                    const {city = "Unknown"} = user;

          section
            h2 Spread and Rest
            .flex
              .half
                h3 Spread Operator
                pre
                  code(data-trim data-noescape).
                    // Arrays
                    const arr1 = [1, 2, 3];
                    const arr2 = [4, 5, 6];
                    const combined = [...arr1, ...arr2];

                    // Objects
                    const obj1 = {a: 1, b: 2};
                    const obj2 = {c: 3, d: 4};
                    const merged = {...obj1, ...obj2};

                    // Function calls
                    const numbers = [1, 2, 3];
                    console.log(Math.max(...numbers));
              .half
                h3 Rest Parameters
                pre
                  code(data-trim data-noescape).
                    function sum(...numbers) {
                      return numbers.reduce((a, b) => a + b, 0);
                    }

                    console.log(sum(1, 2, 3, 4)); // 10

                    // With other parameters
                    function greet(greeting, ...names) {
                      return `${greeting} ${names.join(", ")}`;
                    }

                    console.log(greet("Hello", "John", "Jane"));

          section
            h2 Promises and Async/Await
            .flex
              .half
                h3 Promises
                pre
                  code(data-trim data-noescape).
                    function fetchUser(id) {
                      return new Promise((resolve, reject) => {
                        setTimeout(() => {
                          if (id > 0) {
                            resolve({id, name: `User ${id}`});
                          } else {
                            reject(new Error("Invalid ID"));
                          }
                        }, 1000);
                      });
                    }

                    fetchUser(1)
                      .then(user => console.log(user))
                      .catch(error => console.error(error));
              .half
                h3 Async/Await
                pre
                  code(data-trim data-noescape).
                    async function getUser(id) {
                      try {
                        const user = await fetchUser(id);
                        console.log(user);
                        return user;
                      } catch (error) {
                        console.error(error);
                        throw error;
                      }
                    }

                    // Usage
                    getUser(1);

                    // Multiple async operations
                    async function getMultipleUsers() {
                      const users = await Promise.all([
                        fetchUser(1),
                        fetchUser(2),
                        fetchUser(3)
                      ]);
                      return users;
                    }

          section
            h2 Modules (ES6+)
            .flex
              .half
                h3 Exporting
                pre
                  code(data-trim data-noescape).
                    // math.js
                    export function add(a, b) {
                      return a + b;
                    }

                    export const PI = 3.14159;

                    export default class Calculator {
                      multiply(a, b) {
                        return a * b;
                      }
                    }

                    // Alternative export syntax
                    function subtract(a, b) {
                      return a - b;
                    }
                    export { subtract };
              .half
                h3 Importing
                pre
                  code(data-trim data-noescape).
                    // main.js
                    import Calculator, { add, PI } from './math.js';
                    
                    const calc = new Calculator();
                    console.log(calc.multiply(2, 3)); // 6
                    console.log(add(4, 5)); // 9
                    console.log(PI); // 3.14159

                    // Import all
                    import * as math from './math.js';
                    console.log(math.add(1, 2));

                    // Dynamic imports
                    async function loadMath() {
                      const math = await import('./math.js');
                      return math.add(1, 2);
                    }

        section#collections
          section
            h2 Collections and Data Structures

          section
            h2 Arrays and Methods
            pre
              code(data-trim data-noescape).
                const numbers = [1, 2, 3, 4, 5];

                // Transformation methods
                const doubled = numbers.map(n => n * 2);        // [2, 4, 6, 8, 10]
                const filtered = numbers.filter(n => n > 3);    // [4, 5]
                const sum = numbers.reduce((acc, n) => acc + n, 0); // 15

                // Find methods
                const found = numbers.find(n => n > 3);         // 4
                const foundIndex = numbers.findIndex(n => n > 3); // 3

                // Check methods
                const hasEven = numbers.some(n => n % 2 === 0); // true
                const allPositive = numbers.every(n => n > 0);  // true

                // Modification methods
                numbers.push(6);      // Add to end
                numbers.unshift(0);   // Add to beginning
                numbers.splice(2, 1); // Remove 1 element at index 2

          section
            h2 Sets and Maps
            .flex
              .half
                h3 Set
                pre
                  code(data-trim data-noescape).
                    const uniqueNumbers = new Set([1, 2, 3, 3, 4]);
                    console.log(uniqueNumbers); // Set {1, 2, 3, 4}

                    uniqueNumbers.add(5);
                    uniqueNumbers.delete(1);
                    console.log(uniqueNumbers.has(2)); // true
                    console.log(uniqueNumbers.size);   // 4

                    // Convert to array
                    const arr = [...uniqueNumbers];
              .half
                h3 Map
                pre
                  code(data-trim data-noescape).
                    const userRoles = new Map();
                    userRoles.set("john", "admin");
                    userRoles.set("jane", "user");

                    console.log(userRoles.get("john")); // "admin"
                    console.log(userRoles.has("bob"));  // false
                    console.log(userRoles.size);        // 2

                    // Iterate
                    for (const [user, role] of userRoles) {
                      console.log(`${user}: ${role}`);
                    }

